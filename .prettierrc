singleQuote: true
parser: flow
trailingComma: es5
overrides:
  - files: "*.css"
    options:
      parser: css
  - files: ".babelrc"
    options:
      parser: json
  - files: "package*.json"
    options:
      parser: json
  - files: "*.md"
    options:
      parser: markdown
  - files: "*.json"
    options:
      parser: json

